function LTrim(sTexto)
{
	return sTexto.replace( /^\s*/, "" );
}

function RTrim(sTexto)
{
	return sTexto.replace( /\s*$/, "" );
}

function Trim(sTexto)
{
	return RTrim(LTrim(sTexto));
}

